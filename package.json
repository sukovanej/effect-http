{
  "name": "effect-http",
  "type": "module",
  "version": "0.43.0",
  "description": "High-level declarative HTTP API for effect-ts",
  "homepage": "https://sukovanej.github.io/effect-http",
  "types": "./index.d.cts",
  "main": "./index.cjs",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": {
        "types": "./*.d.ts",
        "default": "./*.js"
      },
      "require": {
        "types": "./*.d.cts",
        "default": "./*.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup && cp package.json dist/ && cp readme.md dist/",
    "watch": "tsc -w -p tsconfig.json",
    "format": "prettier -w ./**/*.{ts,js}",
    "format:check": "pnpm format --check",
    "eslint": "eslint .",
    "lint": "pnpm format:check && pnpm eslint && tsc --noEmit",
    "docs": "docgen",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "version": "changeset version"
  },
  "keywords": [],
  "author": "Milan Suk <Milansuk@email.cz>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@effect/docgen": "^0.3.8",
    "@effect/platform": "^0.34.0",
    "@effect/platform-bun": "^0.23.0",
    "@effect/schema": "^0.53.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.10.4",
    "@types/swagger-ui-dist": "^3.30.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "effect": "2.0.0-next.60",
    "effect-log": "^0.24.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "schema-openapi": "^0.26.0",
    "swagger-ui-dist": "^5.10.3"
  },
  "peerDependencies": {
    "@effect/platform": "^0.34.0",
    "@effect/schema": "^0.53.0",
    "effect": "2.0.0-next.60"
  },
  "optionalDependencies": {
    "@effect/platform-node": "^0.35.0"
  }
}
