{
  "name": "effect-http",
  "type": "module",
  "version": "0.46.0",
  "license": "MIT",
  "author": "Milan Suk <Milansuk@email.cz>",
  "description": "High-level declarative HTTP API for effect-ts",
  "homepage": "https://sukovanej.github.io/effect-http",
  "repository": {
    "type": "git",
    "url": "https://github.com/sukovanej/effect-http.git"
  },
  "bugs": {
    "url": "https://github.com/sukovanej/effect-http/issues"
  },
  "packageManager": "pnpm@8.12.1",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "pnpm build-prepare && pnpm build-esm && pnpm build-cjs && pnpm build-annotate && build-utils pack-v2",
    "build-prepare": "build-utils prepare-v2 && pnpm format",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build --plugins annotate-pure-calls --out-dir build --source-maps",
    "clean": "rimraf build dist docs",
    "watch": "tsc -b -w tsconfig.json",
    "check": "tsc -b tsconfig.json",
    "format": "prettier -w ./**/*.{ts,js}",
    "format:check": "pnpm format --check",
    "eslint": "eslint .",
    "lint": "pnpm format:check && pnpm eslint && tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "version": "changeset version"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@changesets/cli": "^2.27.1",
    "@effect/build-utils": "^0.7.0",
    "@effect/docgen": "^0.3.8",
    "@effect/language-service": "^0.1.0",
    "@effect/platform": "^0.38.0",
    "@effect/platform-bun": "^0.26.0",
    "@effect/platform-node": "^0.38.0",
    "@effect/schema": "^0.54.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.10.5",
    "@types/swagger-ui-dist": "^3.30.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "effect": "2.0.0-next.61",
    "effect-log": "^0.24.4",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "schema-openapi": "^0.26.1",
    "swagger-ui-dist": "^5.10.5"
  },
  "peerDependencies": {
    "@effect/platform": "^0.38.0",
    "@effect/schema": "^0.54.0",
    "effect": "2.0.0-next.61"
  }
}
