{
  "name": "effect-http",
  "type": "module",
  "version": "0.41.0",
  "description": "High-level declarative HTTP API for effect-ts",
  "homepage": "https://sukovanej.github.io/effect-http",
  "types": "./index.d.cts",
  "main": "./index.cjs",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": {
        "types": "./*.d.ts",
        "default": "./*.js"
      },
      "require": {
        "types": "./*.d.cts",
        "default": "./*.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup && cp package.json dist/ && cp readme.md dist/",
    "watch": "tsc -w -p tsconfig.json",
    "format": "prettier -w ./**/*.{ts,js}",
    "format:check": "pnpm format --check",
    "eslint": "eslint .",
    "lint": "pnpm format:check && pnpm eslint && tsc --noEmit",
    "docs": "docgen",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "version": "changeset version"
  },
  "keywords": [],
  "author": "Milan Suk <Milansuk@email.cz>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@effect/docgen": "^0.3.6",
    "@effect/platform": "^0.31.1",
    "@effect/platform-bun": "^0.20.2",
    "@effect/schema": "^0.49.4",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.10.0",
    "@types/swagger-ui-dist": "^3.30.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "effect": "2.0.0-next.56",
    "effect-log": "^0.23.2",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.5.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "schema-openapi": "^0.22.0",
    "swagger-ui-dist": "^5.10.3"
  },
  "peerDependencies": {
    "@effect/platform": "^0.31.0",
    "@effect/schema": "^0.49.0",
    "effect": "2.0.0-next.56"
  },
  "optionalDependencies": {
    "@effect/platform-node": "^0.32.2"
  }
}
