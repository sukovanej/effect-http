{
  "name": "effect-http",
  "version": "0.15.0",
  "description": "High-level declarative HTTP API for effect-ts",
  "homepage": "https://sukovanej.github.io/effect-http",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "post-build": "mv dist/*.cjs . && mkdir -p internal && mv dist/internal/*.cjs ./internal/",
    "watch": "tsc -w -p tsconfig.json",
    "format": "prettier -w src tests examples",
    "format:check": "pnpm format --check",
    "eslint": "eslint .",
    "lint": "pnpm format:check && pnpm eslint && tsc --noEmit",
    "docs-ts": "docs-ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "version": "changeset version"
  },
  "keywords": [],
  "author": "Milan Suk <Milansuk@email.cz>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/coverage-c8": "^0.32.2",
    "docs-ts": "^0.8.0",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@effect/data": "^0.12.9",
    "@effect/io": "^0.27.1",
    "@effect/schema": "^0.21.0",
    "effect-log": "^0.3.10",
    "express": "^4.18.2",
    "schema-openapi": "^0.2.6",
    "swagger-ui-express": "^4.6.3"
  }
}
